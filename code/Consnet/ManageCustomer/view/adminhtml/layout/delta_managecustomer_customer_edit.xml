    <update handle="editor"/>
    <body>
        <referenceContainer name="content">
            <block class="Consnet\ManageCustomer\Block\Adminhtml\Customer\Edit" name="consnet_managecustomer_customer_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Consnet\ManageCustomer\Block\Adminhtml\Customer\Edit\Tabs" name="consnet_managecustomer_customer_tabs">
                <block class="Consnet\ManageCustomer\Block\Adminhtml\Customer\Edit\Tab\Customer" name="consnet_managecustomer_customer_edit_tab_customer"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">customer</argument>
                    <argument name="block" xsi:type="string">consnet_managecustomer_customer_edit_tab_customer</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
